<%= form_for 'questionnaire_response[data]', method: :post, url: questionnaire_responses_path do |f| %>
    <% @questions.each do |key, attrs| %>
        <% f.label key, attrs[:question_text] %>
        <%= render attrs[:type], f: f, key: key, attrs: attrs %>

        <%= end %>
    <br>
        <%= f.submit 'Submit' %>
<%= end %>